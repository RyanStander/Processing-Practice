// fill the rectangle with yellow if the mouse
// is inside rectangle at the middle of the
// screen and black if it is outside the middle.

void setup() {
  size (400,400);
}

void draw() {
  background (0);
  rect (100,100,200,200);
  if (mouseX > width = 200 && mouseY > height = 200); {
    fill (255,255,0);
  } else {
    fill (0);
  }
}
